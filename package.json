{
  "name": "players-api",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug-teams": "node scripts/debugTeams.js",
    "debug-players": "node scripts/debugPlayersTeams.js",
    "debug-player-details": "node scripts/debugPlayerDetails.js",
    "fix-relations": "node scripts/fixTeamRelations.js",
    "fix-players-teams": "node scripts/fixPlayersTeams.js",
    "fix-team-ids": "node scripts/fixTeamIds.js",
    "full-fix": "npm run fix-relations && npm run fix-players-teams && npm run fix-team-ids",
    "diagnose-all": "npm run debug-teams && npm run debug-players && npm run debug-player-details"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "mssql": "^8.1.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}